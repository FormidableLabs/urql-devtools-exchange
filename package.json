{
  "name": "@urql/devtools",
  "version": "0.1.0",
  "description": "The exchange for use with Urql Devtools",
  "main": "dist/urql-devtools-exchange.js",
  "module": "dist/urql-devtools-exchange.es.js",
  "types": "dist/types/index.d.ts",
  "source": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./dist ./node_modules/.cache",
    "build": "rollup -c rollup.config.js",
    "watch": "rollup -w -c rollup.config.js",
    "lint": "eslint . --ext .ts,.tsx",
    "check": "tsc --noEmit",
    "prettier-check": "prettier -c src/** README.md",
    "prepublishOnly": "run-s clean build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FormidableLabs/urql-devtools-exchange.git"
  },
  "keywords": [
    "devtools",
    "urql",
    "graphql",
    "exchange"
  ],
  "author": "Formidable",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/urql-devtools-exchange/issues"
  },
  "homepage": "https://github.com/FormidableLabs/urql-devtools-exchange#readme",
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-plugin-closure-elimination": "^1.3.0",
    "babel-plugin-transform-async-to-promises": "^0.8.14",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "graphql": "^14.4.2",
    "husky": "^3.0.3",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.22.1",
    "terser": "^4.1.4",
    "ts-jest": "^24.2.0",
    "typescript": "^3.5.3",
    "urql": "^1.2.0"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "urql": ">= 1.2.0"
  },
  "dependencies": {
    "wonka": "^3.2.1"
  }
}
