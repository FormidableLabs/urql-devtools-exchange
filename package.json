{
  "name": "@urql/devtools",
  "version": "0.1.2",
  "description": "The exchange for use with Urql Devtools",
  "main": "dist/urql-devtools-exchange.js",
  "module": "dist/urql-devtools-exchange.es.js",
  "types": "dist/types/index.d.ts",
  "source": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./dist ./node_modules/.cache",
    "build": "rollup -c rollup.config.js",
    "watch": "rollup -w -c rollup.config.js",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "check": "tsc --noEmit",
    "prettier-check": "prettier -c src/** README.md",
    "prepublishOnly": "run-s clean build",
    "changelog": "docker run -it --rm -v \"$(pwd)\":/usr/local/src/your-app ferrarimarco/github-changelog-generator -u FormidableLabs -p urql-devtools-exchange"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FormidableLabs/urql-devtools-exchange.git"
  },
  "keywords": [
    "devtools",
    "urql",
    "graphql",
    "exchange"
  ],
  "author": "Formidable",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/urql-devtools-exchange/issues"
  },
  "homepage": "https://github.com/FormidableLabs/urql-devtools-exchange#readme",
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.31",
    "@types/react": "^16.9.25",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "@urql/core": "^1.10.7",
    "babel-plugin-closure-elimination": "^1.3.0",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "graphql": "^14.6.0",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react": "^16.9.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "terser": "^4.6.7",
    "ts-jest": "^24.2.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "@urql/core": ">= 1.9.0"
  },
  "dependencies": {
    "wonka": "^4.0.9"
  }
}
